<!DOCTYPE html>

    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - Dsign Elixer</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        <meta name="description" content="">
        <meta name="author" content="HalfcoreDev.com" />
        <link href='http://fonts.googleapis.com/css?family=Rye' rel='stylesheet' type='text/css'>
        <style>
            h2{color:#b12e1c; font-weight: bold; font-family: Times New Roman}
        </style>
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
        <link href="~/Content/admin.css" rel="stylesheet" />
        
    </head>
<body>

        
    @{
        Html.RenderPartial("_Header");
    }
    
    <div class="container">
        <div class="sixteen columns" >
            @RenderBody()
        </div>
        
        
 </div>
    
    
    
     
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)    
    
    <script src="~/Scripts/jquery-ui-1.10.3.js"></script>
    <script src="~/Scripts/fancybox/jquery.fancybox.pack.js"></script>
    <link href="~/Scripts/fancybox/jquery.fancybox-thumbs.css" rel="stylesheet" />
    <script src="~/Scripts/fancybox/jquery.fancybox-thumbs.js"></script>
    <link href="~/Scripts/fancybox/jquery.fancybox-buttons.css" rel="stylesheet" />
    <script src="~/Scripts/fancybox/jquery.fancybox-buttons.js"></script>
    <script src="~/Scripts/audiojs/audio.min.js"></script>
    <link href="~/Scripts/datatables/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/Scripts/datatables/css/jquery-ui-1.10.3.custom.css" rel="stylesheet" />
    <link href="~/Scripts/datatables/css/jquery-ui-1.10.3.custom.min.css" rel="stylesheet" />
    <script src="~/Scripts/datatables/jquery.dataTables.js"></script>
    <script src="~/Scripts/datatables/jquery.dataTables.rowReordering.js"></script>
   
    
</body>
</html>
